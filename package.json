{
  "name": "ClevertapProject",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "clear-watchman": "watchman watch-del-all",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "yarn clear-watchman && react-native start --reset-cache",
    "fresh": "rm -rf yarn.lock && rm -rf node_modules && cd ios && rm -rf Podfile.lock && pod deintegrate && cd .. && yarn && cd ios && pod install && cd .. && cd android && ./gradlew clean && cd ..",
    "test": "jest",
    "lint": "eslint ."
  },
  "dependencies": {
    "react": "18.1.0",
    "react-native": "0.70.7"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@tsconfig/react-native": "^2.0.3",
    "@types/jest": "^29.5.0",
    "@types/react": "^18.0.33",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "0.72.3",
    "react-test-renderer": "18.1.0",
    "typescript": "^5.0.3"
  },
  "jest": {
    "preset": "react-native"
  }
}
